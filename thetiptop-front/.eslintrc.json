{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": [
      "google",
      "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "object-curly-spacing": ["error", "always"],
    "quotes": ["error"],
    "require-jsdoc": 0
  }
}
